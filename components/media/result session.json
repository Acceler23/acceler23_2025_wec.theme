{
  "ComponentName": "result_session",
  "BlockType": "stack",
  "HorizontalAlignment": "Right",
  "Width": "{TableWidth}",
  "Height": "{TableHeight}",
  "BackgroundImage": "Session/Session_bg.png",
  "BackgroundImageOpacity": 60,
  "ColorizeBackground": {
    "Color": "{DarkBlue}"
  },
  "Orientation": "Vertical",
  "Items": [
    {
      "BlockType": "stack",
      "Orientation": "Vertical",
      "HorizontalAlignment": "Center",
      "Width": "{TableWidth}",
      //"Height": "{TableHeight}",
      "BackgroundImage": "Session/table_shade.png",
      "ColorizeBackground": {
        "Color": "{LightBlue}"
      },
      "Items": [
        {
          "BlockType": "component",
          "Component": "session_head",
          "Margin": "120,60,60,0",
          "ComponentOptions": {
            "Vars": {
              "sessionInfo": "{Session.SessionType}"
            }
          }
        },
        {
          "BlockType": "shape",
          "Margin": "120,15,120,0",
          "Width": 1160,
          "Height": 5,
          "Opacity": 30,
          "ShapeOptions": {
            "Fill": "{LightBlue}",
            "ShapeType": "rectangle"
          }
        },
        {
          "BlockType": "dock",
          "Orientation": "Horizontal",
          "Width": "*",
          "Margin": "60,40,70,0",
          "Items": [
            {
              "BlockType": "text",
              "Source": "[{Session.RacingClass.Name}]",
              "VerticalAlignment": "Center",
              "HorizontalAlignment": "Left",
              "FontSize": 25,
              "Padding": "5,10,5,0",
              "FontName": "{CardHeadFont}",
              "Background": "{Session.RacingClass.Color}",
              "Foreground": "{Session.RacingClass.SecondaryColor}",
              "TextOptions": {
                "VerticalAlignment": "Bottom"
              }
            },
            {
              "BlockType": "text",
              "HorizontalAlignment": "Right",
              "Source": "[{Session.Track.CircuitFullName, Converter=StringToUpperString}]",
              "VerticalAlignment": "Center",
              "FontSize": 25,
              "Padding": "5",
              "FontName": "{CardHeadFont}",
              "Triggers": [
                {
                  "Trigger": "NameDescription"
                }
              ]
            }
          ]
        },
        {
          "BlockType": "table",
          "Margin": "60,15,60,0",
          "Height": 620,
          "Triggers": [
            {
              "Trigger": "TableParts"
            }
          ],
          "TableOptions": {
            "ItemsSource": "{Session.Drivers}",
            "IndexStart": 0,
            "IndexEnd": 9,
            "RowSpacing": 15,
            "HeaderTemplate": {
              "BlockType": "text"
            },
            "Columns": [
              {
                "Header": "",
                "Template": {
                  "BlockType": "component",
                  "Component": "table_position"
                }
              },
              {
                "Header": "",
                "Template": {
                  "BlockType": "component",
                  "Component": "table_logo"
                }
              },
              {
                "Header": "",
                "Template": {
                  "BlockType": "component",
                  "Component": "table_number"
                }
              },
              {
                "Header": "",
                "Template": {
                  "BlockType": "component",
                  "Component": "table_car",
                  "ComponentOptions": {
                    "Vars": {
                      "renderCar1": "<renderCar>"
                    }
                  }
                }
              },
              {
                "Header": "",
                "Template": {
                  "BlockType": "component",
                  "Component": "table_driver",
                  "ComponentOptions": {
                    "Vars": {
                      "renderDriver1": "<renderDriver>"
                    }
                  }
                }
              },
              {
                "Header": "",
                "Template": {
                  "BlockType": "component",
                  "Component": "table_team",
                  "ComponentOptions": {
                    "Vars": {
                      "renderTeam1": "<renderTeam>"
                    }
                  }
                }
              },
              {
                "Header": "",
                "Template": {
                  "BlockType": "component",
                  "Component": "table_session",
                  "ComponentOptions": {
                    "Vars": {
                      "sessionInfo": "{Item.Time, Converter=StringToUpperString}"
                    }
                  }
                }
              }
            ]
          }
        },
        {
          "BlockType": "component",
          "Component": "fastest_lap",
          "Height": "{TableLineHeight}"
        },
        {
          "BlockType": "component",
          "Component": "bottom_table",
          "Triggers": [
            {
              "Condition": "{Session.SessionType, Converter=StringNotEquals, Parameter=Race}",
              "Property": "MarginTop",
              "Value": 80
            }
          ]
        }
      ]
    }
  ]
}
