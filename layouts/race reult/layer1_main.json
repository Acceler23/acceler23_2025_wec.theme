{
    "BlockRoot": {
        "Name": "mainStack",
        "BlockType": "dock",
        "Width": "{RenderWidth}",
        "Height": "{RenderHeight}",
        "Margin": "80,60,90,60",
        "Items": [
            {
                "BlockType": "component",
                "Component": "leader_driver_card",
                "HorizontalAlignment": "Left",
                "ComponentOptions": {
                    "Vars": {
                        "sessionLeader": "WINNER",
                        "fontSize": 70,
                        "sessionInfo": "{Item.Team.Name, Converter=StringToUpperString}",
                        "sessionColor": "{Item.Team.SecondaryColor}",
                        "sessionColor2": "{Item.Team.Color}"
                    }
                }
            },
            {
                "BlockType": "component",
                "Component": "result_session",
                "ComponentOptions": {
                    "Vars": {
                        "renderDriver": true,
                        "renderTeam": false,
                        "renderCar": false
                    }
                }
            }
        ]
    }
}